@page
@model ASPNetGerenciadorVeiculos.Pages.Veiculos.IndexModel
@{
    ViewData["Title"] = "Cadastro de usuários";
}
<h2>@ViewData["Title"]</h2>
<p>
    <a class="btn btn-primary" asp-page="/Usuarios/Criar">Cadastrar Veículo</a>
</p>
<p>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Usuário</td>
            <td>Senha</td>
            <td>E-mail</td>
        </tr>
    </thead>
    <tbody>
            @foreach(var veiculo in Model.Veiculos)
            {
                <tr>
                    <th>@veiculo.Id</th>
                    <th>@veiculo.Nome</th>
                    <th>@veiculo.Modelo</th>
                    <th>@veiculo.Marca</th>
                    <th>@veiculo.Renavam</th>
                    <th>@veiculo.AnoFabricacao</th>
                    <th>@veiculo.AnoModelo</th>
                    <th>@veiculo.DirFoto</th>
                </tr>
            }
        </tbody>
</table>
</p>